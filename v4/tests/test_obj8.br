func main() {
  x = @;
  y = @;
  if (x == x) { print("1"); }
  if (x == nil) { print("2"); } else { print("3"); }
  if (x != x) { print("4"); }
  if (nil == x) { print("5"); } else { print("6"); }
  if (x == y) { print("7"); }
  if (x != y) { print("8"); } else { print("9"); }
}

/*
*OUT*
1
3
6
8
